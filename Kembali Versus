import java.util.*;
import java.lang.Math;

public class Solution {
    public static void main(String[] args) {
      int angka, balik = 0;
      Scanner input = new Scanner(System.in);
      angka = input.nextInt();
      while( angka != 0 )
      {
          balik = balik * 10;
          balik = balik + angka%10;
          angka = angka/10;
      }
      System.out.println("Kebalikan dari angka tersebut adalah : " + balik);
   }
}
